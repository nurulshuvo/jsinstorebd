<div id="msg" >

<table id="payment">
  <thead>
  <tr>
    <th>Cart</th>
    <th>Status</th>
    <th>Transaction</th>
    <th></th>
  </tr>
</thead>
<tbody>
<% @payment_notifications.each do |payment_notification| %>
  <tr>
    <td><%= payment_notification.cart_id %></td>
    <td><%= payment_notification.status %></td>
    <td><%= payment_notification.transaction_id %></td>
    <td><%= link_to 'Delete', payment_notification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</tbody>
</table>
</div>